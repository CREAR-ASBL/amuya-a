                                                         <children>
                                                            <Label text="Algebraic formulation:" />
                                                            <Label fx:id="lblPositiveFormulation" text="-formulation-" />
                                                         </children>
                                                      </HBox>
                                                      <HBox layoutX="10.0" layoutY="25.0" prefWidth="200.0">
                                                         <children>
                                                            <Label text="Propositional formulation:" />
                                                            <TextField fx:id="ttfdPositiveFormulation" text="positive" HBox.hgrow="ALWAYS" />
                                                         </children>
                                                      </HBox>
                                                      <HBox layoutX="10.0" layoutY="40.0" prefWidth="200.0">
                                                         <children>
                                                            <Label text="Description:" />
                                                            <TextArea fx:id="ttaaPositiveDescription" prefHeight="100.0" text="comment" wrapText="true" HBox.hgrow="ALWAYS" />
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                </VBox>
                                                <VBox>
                                                   <children>
                                                      <Label styleClass="subtitle" text="Negative contradictional conjunction" />
                                                      <HBox>
                                                         <children>
                                                            <Label text="Algebraic formulation:" />
                                                            <Label fx:id="lblNegativeFormulation" text="-formulation-" />
                                                         </children>
                                                      </HBox>
                                                      <HBox layoutX="10.0" layoutY="25.0" prefWidth="200.0">
                                                         <children>
                                                            <Label text="Propositional formulation:" />
                                                            <TextField fx:id="ttfdNegativeFormulation" text="negative" HBox.hgrow="ALWAYS" />
                                                         </children>
                                                      </HBox>
                                                      <HBox layoutX="10.0" layoutY="40.0" prefWidth="200.0">
                                                         <children>
                                                            <Label text="Description:" />
                                                            <TextArea fx:id="ttaaNegativeDescription" prefHeight="100.0" text="desc" wrapText="true" HBox.hgrow="ALWAYS" />
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                </VBox>
                                                <VBox layoutX="20.0" layoutY="175.0">
                                                   <children>
                                                      <Label styleClass="subtitle" text="Symmetric contradictional conjunction" />
                                                      <HBox>
                                                         <children>
                                                            <Label text="Algebraic formulation:" />
                                                            <Label fx:id="lblSymmetricFormulation" text="-formulation-" />
                                                         </children>
                                                      </HBox>
                                                      <HBox layoutX="10.0" layoutY="25.0" prefWidth="200.0">
                                                         <children>
                                                            <Label text="Propositional formulation:" />
                                                            <TextField fx:id="ttfdSymmetricFormulation" text="symmetric" HBox.hgrow="ALWAYS" />
                                                         </children>
                                                      </HBox>
                                                      <HBox layoutX="10.0" layoutY="40.0" prefWidth="200.0">
                                                         <children>
                                                            <Label text="Description:" />
                                                            <TextArea fx:id="ttaaSymmetricDescription" prefHeight="100.0" text="test" wrapText="true" HBox.hgrow="ALWAYS" />
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                </VBox>
                                             </children>
                                          </VBox>
                                       </children>
                                    </VBox>
                                 </children>
                              </HBox>
                           </content>
                           <font>
                              <Font size="20.0" />
                           </font>
                        </TitledPane>
                     </children>
                  </VBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </content>
</ScrollPane>
