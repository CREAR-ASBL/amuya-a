                                                   <children>
                                                      <ListView fx:id="ltvwLogicSystem" prefHeight="100.0" HBox.hgrow="ALWAYS" />
                                                      <VBox alignment="CENTER">
                                                         <children>
                                                            <ComboBox fx:id="cobxLogicSystem" disable="true" prefWidth="150.0" />
                                                            <HBox>
                                                               <children>
                                                                  <Button fx:id="bnRemoveLogicSystem" disable="true" mnemonicParsing="false" onAction="#removeLogicSystem" text="Remove" />
                                                                  <Button fx:id="bnAddLogicSystem" disable="true" mnemonicParsing="false" onAction="#addLogicSystem" text="Add" />
                                                               </children>
                                                            </HBox>
                                                         </children>
                                                      </VBox>
                                                   </children>
                                                </HBox>
                                             </children>
                                          </VBox>
                                          <VBox>
                                             <children>
                                                <Label text="Is deduced from Contradictional Implications:" wrapText="true" />
